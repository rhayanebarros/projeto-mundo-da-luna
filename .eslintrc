{
    "extends": "airbnb-base",
    "rules": {
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true,
          "optionalDependencies": true,
          "peerDependencies": false
        }
      ],
      "linebreak-style": 0,
      "no-unused-expressions": [
        "error",
        {
          "allowTernary": true
        }
      ]
    },
    "env": {
      "browser": true,
      "jquery": true
    }
  }